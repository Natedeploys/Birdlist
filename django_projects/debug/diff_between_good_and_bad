-            q.email_box_interval = 0
-
-
-        queue_time_delta = timedelta(minutes=q.email_box_interval)
-
-        if (q.email_box_last_check + queue_time_delta) > datetime.now():
-            continue
-
-        process_queue(q)
-
-        q.email_box_last_check = datetime.now()
-        q.save()
