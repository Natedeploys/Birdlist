-    regex = re.compile("^\[[A-Za-z0-9]+-\d+\]")
-    if regex.match(subject):
-        # This is a reply or forward.
-        ticket = re.match(r"^\[(?P<queue>[A-Za-z0-9]+)-(?P<id>\d+)\]", subject).group('id')
-    else:
-        ticket = None
-
