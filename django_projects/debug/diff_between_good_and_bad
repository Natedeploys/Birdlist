-class TicketForm(forms.Form):
-    queue = forms.ChoiceField(
-        label=_('Queue'),
-        required=True,
-        choices=()
-        )
-
-    title = forms.CharField(
-        max_length=100,
-        required=True,
-        widget=forms.TextInput(),
-        label=_('Summary of the problem'),
-        )
