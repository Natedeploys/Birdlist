-        for num in data[0].split():
-            status, data = server.fetch(num, '(RFC822)')
-            ticket_from_message(message=data[0][1], queue=q)
-            server.store(num, '+FLAGS', '\\Deleted')
-        server.expunge()
-        server.close()
-        server.logout()
