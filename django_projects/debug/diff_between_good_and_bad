-def process_email():
-    for q in Queue.objects.filter(
-            email_box_type__isnull=False,
-            allow_email_submission=True):
-
-        if not q.email_box_last_check:
-            q.email_box_last_check = datetime.now()-timedelta(minutes=30)
-
