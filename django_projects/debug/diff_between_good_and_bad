diff --git a/django_projects/do_not_use_was_testsite/songbird_db/admin.py b/django_projects/do_not_use_was_testsite/songbird_db/admin.py
new file mode 100644
index 0000000..ebeabf6
--- /dev/null
+++ b/django_projects/do_not_use_was_testsite/songbird_db/admin.py
@@ -0,0 +1,50 @@
+from testsite.songbird_db.models import Post, Comment, Neuron 
+from django.contrib import admin
+
+class CommentInline(admin.TabularInline):
+    model = Comment
+    fields = ('content',)
+
+class PostModelAdmin(admin.ModelAdmin):
+
+    fields= ('content',)
+    inlines = [CommentInline]
+
+    def save_model(self, request, form, change): 
+
+        instance = form.save(commit=False)
+
+        instance.user = request.user
+
+        instance.save()
+
+        form.save_m2m()
+
+        return instance
+
+    def save_formset(self, request, form, formset, change): 
+
+        def set_user(instance):
+            instance.user = request.user
+            instance.save()
+
+        if formset.__name__ == 'CommentInline':
+            instances = formset.save(commit=False)
+            map(set_user, instances)
+            formset.save_m2m()
+            return instances
+        else:
+            return formset.save()
+
+#class NeuronAdmin(admin.ModelNeuron):
+#    list_display = ['name', 'bird']
+#    prepopulated_fields = {
+#        'slug': ('name', 'bird')
+#    }
+
+#admin.site.register(Neuron, NeuronAdmin)
+
+
+
+admin.site.register(Post, PostModelAdmin)
+
diff --git a/django_projects/do_not_use_was_testsite/songbird_db/models.py b/django_projects/do_not_use_was_testsite/songbird_db/models.py
new file mode 100644
index 0000000..1eb10be
--- /dev/null
+++ b/django_projects/do_not_use_was_testsite/songbird_db/models.py
@@ -0,0 +1,60 @@
+from django.db import models
+from django.contrib.auth.models import User
+
+class PrincipleInvestigator(models.Model):
+    name = models.CharField(max_length=30)
+    email = models.EmailField()
+    data_path =  models.CharField(max_length=3000)
+ 
+    def __unicode__(self):
+        return self.name
+
+#    class Admin:
+#        pass
+
+
+class Bird(models.Model):
+    name = models.CharField(max_length=30)
+    birth_day = models.DateField()
+    data_path =  models.CharField(max_length=3000, blank=True, null=True) 
+    pi = models.ForeignKey(PrincipleInvestigator)
+    created_by = models.ForeignKey(User) 
+   
+    def __unicode__(self):
+        return self.name
+
+#    class Admin:
+#        pass
+
+
+class Neuron(models.Model):
+    name = models.CharField(max_length=100)
+    bird = models.ManyToManyField(Bird)
+    pi = models.ForeignKey(PrincipleInvestigator)
+    recording_date = models.DateField()
+    name_raw_file = models.CharField(max_length=1000)
+    name_sorted_file = models.CharField(max_length=1000, blank=True, null=True)
+    data_path =  models.CharField(max_length=3000, blank=True, null=True)
+    exclude_from_analysis = models.BooleanField()
+    comment = models.TextField(max_length=2048, blank=True, null=True)
+
+    def bird_name(self):
+        return self.bird.all()
+
+    def __unicode__(self):
+        return self.name
+    
+#    class Admin:
+#        list_display = ('name', 'bird_name')
+#	list_filter = ('pi', 'exclude_from_analysis', 'pi')
+
+class Post(models.Model):
+    user = models.ForeignKey(User)
+    content = models.TextField()
+
+class Comment(models.Model):
+    post = models.ForeignKey(Post)
+    user = models.ForeignKey(User)
+    content = models.TextField()
+
+
