-                'filename': name,
-                'content': part.get_payload(decode=True),
-                'type': part.get_content_type()},
-                )
-
-        counter += 1
-
-    now = datetime.now()
-
-    if ticket:
-        try:
-            t = Ticket.objects.get(id=ticket)
-            new = False
-        except Ticket.DoesNotExist:
-            ticket = None
-
