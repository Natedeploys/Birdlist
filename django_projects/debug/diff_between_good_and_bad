-    cursor = connection.cursor()
-    cursor.execute(sql)
-    report_output = query_to_dict(cursor.fetchall(), cursor.description)
-
-    data = []
-
-    for record in report_output:
-        line = []
-        for c in columns:
-            line.append(record[c])
-        data.append(line)
-
